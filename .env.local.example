AGENT_PLATFORM_GEMINI_COMMON_STORE_NAME=

# Freshdesk API 설정
AGENT_PLATFORM_FRESHDESK_DOMAIN=
AGENT_PLATFORM_FRESHDESK_API_KEY=

# Fly.io Redis 애드온 사용 시 주석 해제하고 실제 URL로 교체하세요
